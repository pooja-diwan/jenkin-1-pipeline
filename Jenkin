                    echo "QA started"
                    sleep 2
                    echo "done"
                }
            } 
            stage ('deploy') {
                steps {
                    echo "deploy started"
                    sleep 2
                    echo "done"
                }
            }
            stage ('monitor') {
                steps {
                    echo "monitor started"
                }
            }
        }
